generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id      Int    @id @default(autoincrement())
  name    String
  email   String   
  card_id String  @unique

  @@map("users")
}

model UsageHistory {
  id        Int      @id @default(autoincrement())
  Timestamp DateTime @default(now())
  name      String
  status    String
  card_id   String   @unique

  @@map("usage_history")
}

model CardIdDumps {
  id      Int    @id @default(autoincrement())
  card_id String @unique

  @@map("card_id_dumps")
}

model LockedStatus {
  id     Int     @id @default(autoincrement())
  status String

  @@map("locked_status")
}
